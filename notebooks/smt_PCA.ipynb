    "from scipy.interpolate import BSpline\n",
    "def smt_pca(dat):\n",
    "    x = np.array(range(dat.shape[-1]))\n",
    "    xnew = np.linspace(x.min(), x.max(), 1000)  \n",
    "    for i in range(dat.shape[0]):\n",
    "        pc_10ms_smt[i] = BSpline(x, dat[i], xnew)\n",
    "    return pc_10ms_smt"
   ]
  }
